@model ClienteModel
@{
    ViewData["Title"] = "Registrar";
}

<h2>Registrar Cliente</h2>

<form asp-controller="Home" asp-action="Registrar">
    <div class="row">
        <div class="col-lg-3">
            <label>Nome:</label>
            <input asp-for="Nome" class="form-control" />
        </div>

        <div class="col-lg-3">
            <label>Data Cadastro:</label>
            <input asp-for="Data_Cadastro" class="form-control" />
        </div>

        <div class="col-lg-3">
            <label>CPF ou CNPJ:</label>
            <input asp-for="Cpf_Cnpj" class="form-control" />
        </div>

        <div class="col-lg-3">
            <label>Data Nascimento:</label>
            <input asp-for="Data_Nascimento" class="form-control" />
        </div>

        <div class="col-lg-3">
            <label>Tipo:</label>
            <input asp-for="Tipo" class="form-control" />
        </div>

        <div class="col-lg-3">
            <label>Telefone:</label>
            <input asp-for="Telefone" class="form-control" />
        </div>

        <div class="col-lg-3">
            <label>Email:</label>
            <input asp-for="Email" class="form-control" />
        </div>

        <div class="col-lg-3">
            <label>Cep:</label>
            <input asp-for="Cep" class="form-control" />
        </div>

        <div class="col-lg-3">
            <label>Logradouro:</label>
            <input asp-for="Logradouro" class="form-control" />
        </div>

        <div class="col-lg-3">
            <label>Número:</label>
            <input asp-for="Numero" class="form-control" />
        </div>

        <div class="col-lg-3">
            <label>Bairro:</label>
            <input asp-for="Bairro" class="form-control" />
        </div>

        <div class="col-lg-3">
            <label>Complemento:</label>
            <input asp-for="Complemento" class="form-control" />
        </div>

        <div class="col-lg-3">
            <label>Cidade:</label>
            <input asp-for="Cidade" class="form-control" />
        </div>

        <div class="col-lg-3">
            <label>Uf:</label>
            <input asp-for="Uf" class="form-control" />
        </div>
    </div>
    <br/>
    <button type="submit" class="btn btn-block btn-primary" >Registrar Cliente</button>
</form>